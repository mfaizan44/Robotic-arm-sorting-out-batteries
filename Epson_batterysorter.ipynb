   "source": [
    "import socket\n",
    "from time import sleep\n",
    "import cv2\n",
    "from ultralytics import YOLO\n",
    "\n",
    "# Constants\n",
    "ip_address = \"192.168.150.2\"  # Change to the real robot IP\n",
    "#ip_address = \"127.0.0.1\"     # Change to simulation IP\n",
    "MODEL_REF = 'C:/Users/megha/OneDrive/Desktop/Sorting_Batteries_yolov8/best.pt'\n",
    "\n",
    "# Battery type placement coordinates by label indices\n",
    "BATTERY_LOCATIONS = {\n",
    "    0: {\"name\": \"AA\", \"pickup\": (-461.502, 178.536, 286.174, -86.211), \"dropoff\": (-338.704, 30.595, 393.445, -86.211)},\n",
    "    1: {\"name\": \"D type\", \"pickup\": (-461.502, 178.536, 304.174, -86.211), \"dropoff\": (-616.241, 30.595, 393.445, -86.211)},\n",
    "    2: {\"name\": \"9v\", \"pickup\": (-461.502, 178.536, 290.174, -86.211), \"dropoff\": (-476.709, 30.595, 393.445, -86.211)}\n",
    "}\n",
    "\n",
    "#safe_position = (-461.502, 178.536, 420, -86.211)\n",
    "\n",
    "# Create a client socket\n",
    "clientSocket = socket.socket(socket.AF_INET, socket.SOCK_STREAM)\n",
    "# Connect to the EPSON robot\n",
    "clientSocket.connect((ip_address, 2001))\n",
    "\n",
    "def epsonCommand(coordinates):\n",
    "    print(f\"Executing: {coordinates}\")\n",
    "    clientSocket.send(coordinates.encode())\n",
    "    confirmation = clientSocket.recv(1024)\n",
    "    print(\"Robot response:\", confirmation.decode())\n",
    "    sleep(1)\n",
    "\n",
    "def detect_and_move(model):\n",
    "    # Initialize the video capture object\n",
    "    cap = cv2.VideoCapture(1)  # '0' is usually the default camera (#C:/Users/megha/OneDrive/Desktop/Sorting_Batteries_yolov8/test1.jpg')\n",
    "    if not cap.isOpened():\n",
    "        print(\"Error: Cannot open camera\")\n",
    "        return\n",
    "\n",
    "    try:\n",
    "        while True:\n",
    "            ret, frame = cap.read()\n",
    "            if not ret:\n",
    "                print(\"Error: Can't receive frame (stream end?). Exiting ...\")\n",
    "                break\n",
    "\n",
    "            # Convert the image from BGR to RGB\n",
    "            image = cv2.cvtColor(frame, cv2.COLOR_BGR2RGB)\n",
    "\n",
    "            # Detect batteries\n",
    "            results = model.predict(image)\n",
    "            for box in results[0].boxes:\n",
    "                label_index = box.cls[0].item()  # Extract label index\n",
    "                if label_index in BATTERY_LOCATIONS:\n",
    "                    battery = BATTERY_LOCATIONS[label_index]\n",
    "                    #Home position\n",
    "                    command = f\"GO {-182.876} {475} {390} {-86.211}\\r\\n\"\n",
    "                    epsonCommand(command)\n",
    "                    print(f\"In Home position\")\n",
    "                    sleep(3)\n",
    "                    # Move to pickup location\n",
    "                    pickup_coords = battery['pickup']\n",
    "                    command = f\"GO {pickup_coords[0]} {pickup_coords[1]} {pickup_coords[2]} {pickup_coords[3]}\\r\\n\"\n",
    "                    epsonCommand(command)\n",
    "                    sleep(3)\n",
    "                    command = f\"Pick\\r\\n\"\n",
    "                    epsonCommand(command)\n",
    "                    # Simulate picking up the battery\n",
    "                    print(f\"Picked up {battery['name']} battery\")\n",
    "                    #safe position\n",
    "                    command = f\"GO {-461.502} {178.536} {420} {-86.211}\\r\\n\"\n",
    "                    epsonCommand(command)\n",
    "                    print(f\"In Safe position\")\n",
    "                    sleep(3)\n",
    "                    # Move to dropoff location\n",
    "                    dropoff_coords = battery['dropoff']\n",
    "                    command = f\"GO {dropoff_coords[0]} {dropoff_coords[1]} {dropoff_coords[2]} {dropoff_coords[3]}\\r\\n\"\n",
    "                    epsonCommand(command)\n",
    "                    sleep(3)\n",
    "                    command = f\"Drop\\r\\n\"\n",
    "                    epsonCommand(command)\n",
    "                    # Simulate dropping off the battery\n",
    "                    print(f\"Dropped off {battery['name']} battery\")\n",
    "                    #Home position\n",
    "                    command = f\"GO {-182.876} {475} {390} {-86.211}\\r\\n\"\n",
    "                    epsonCommand(command)\n",
    "                    print(f\"In Home position\")\n",
    "                    sleep(3)\n",
    "\n",
    "            # Display the resulting frame\n",
    "            cv2.imshow('Battery Detection', cv2.cvtColor(image, cv2.COLOR_RGB2BGR))\n",
    "            if cv2.waitKey(1) & 0xFF == ord('q'):\n",
    "                break\n",
    "    finally:\n",
    "        # When everything done, release the capture\n",
    "        cap.release()\n",
    "        cv2.destroyAllWindows()\n",
    "\n",
    "def main():\n",
    "    # Load the YOLOv8 model\n",
    "    model = YOLO(MODEL_REF)\n",
    "    model.conf = 0.5  # Set the confidence threshold\n",
    "\n",
    "    # Start detection and moving sequence\n",
    "    detect_and_move(model)\n",
    "\n",
    "    # Close the socket connection\n",
    "    clientSocket.close()\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    main()\n"
   ]
  },
